<?xml version="1.0"?>
<rdf:RDF xmlns="new2.com/#"
     xml:base="new2.com/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="new2.com/#"/>
	
    <owl:ObjectProperty rdf:about="new2.com/#hasDuration"/>
	
    <owl:ObjectProperty rdf:about="new2.com/#hasPointInTime"/>
	
    <owl:Class rdf:about="new2.com/#Chronoid">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="new2.com/#projectsTo"/>
                </owl:oneOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
	
    <owl:Class rdf:about="new2.com/#PointInTime">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="new2.com/#hasPointInTime"/>
                <owl:someValuesFrom rdf:resource="new2.com/#PointInTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
	
    <owl:Class rdf:about="new2.com/#Process"/>
	
    <owl:Class rdf:about="new2.com/#TimeInterval">
        <rdfs:subClassOf rdf:resource="new2.com/#Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="new2.com/#hasDuration"/>
                <owl:someValuesFrom rdf:resource="new2.com/#TimeInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
	
    <owl:NamedIndividual rdf:about="new2.com/#projectsTo"/>
</rdf:RDF>